{
  "name": "genshin-builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "generate-all": "npm run generate-sitemap && npm run generate-common-dict",
    "generate-sitemap": "node ./scripts/sitemap.mjs",
    "generate-common-dict": "node ./scripts/generate-common-dict.js",
    "analize": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "type-check": "tsc",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./db/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "now-build": "pnpm db:generate --schema=./db/schema.prisma && next build"
  },
  "dependencies": {
    "@freestar/pubfig-adslot-react-component": "^3.6.0",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "@next/mdx": "^13.4.19",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-table": "^8.9.3",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.2",
    "genshin-data": "^0.33.0",
    "gray-matter": "^4.0.3",
    "hsr-data": "^0.3.0",
    "leaflet": "^1.9.4",
    "nanostores": "^0.9.3",
    "next": "^13.4.19",
    "next-mdx-remote": "^4.4.1",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-lazy-load-image-component": "^1.6.0",
    "react-leaflet": "^4.2.1",
    "react-tooltip": "^5.21.1",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "swr": "^2.2.2",
    "tof-builds": "^0.8.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@next/env": "^13.4.19",
    "@types/leaflet": "^1.9.4",
    "@types/node": "^18.17.13",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-lazy-load-image-component": "^1.5.3",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "eslint": "8.47.0",
    "eslint-config-next": "^13.4.19",
    "globby": "^13.2.2",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  }
}
