    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "@nanostores/persistent": "^0.5.3",
    "@nanostores/react": "^0.2.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.11",
    "genshin-data": "^0.20.1",
    "gray-matter": "^4.0.3",
    "nanostores": "^0.5.12",
    "next": "^12.1.6",
    "next-sitemap": "^3.0.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-lazy-load-image-component": "^1.5.4",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@types/node": "^14.18.21",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^12.1.6",
    "globby": "^13.1.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.1",
    "typescript": "^4.7.3"
  }
