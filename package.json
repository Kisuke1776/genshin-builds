{
  "name": "genshin-builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
